#: index.docbook:595
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guimenuitem>Batch</guimenuitem> <guimenuitem>Filter images</guimenuitem>"
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "This plugin can be used for batch filter images in an Album. It use \"convert\" program from \"ImageMagick\" package. An image effect preview (partial or full) is available. 9 filters types have been implemented:"
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Add noise</emphasis> : add noise in an image."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Antialias</emphasis> : remove pixel aliasing."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Blur</emphasis> : blur the image with a Gaussian operator."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Despeckle</emphasis> : reduce the speckles within an image."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Enhance</emphasis> : apply a digital filter to enhance a noisy image."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Median</emphasis> : apply a median filter to an image."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Noise reduction</emphasis> : reduce noise in an image."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Sharpen</emphasis> : sharpen the image with a Gaussian operator."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "<emphasis>Unsharp</emphasis> : sharpen the image with an unsharp mask operator."
msgstr ""

#: index.docbook:595
#, no-c-format
msgid "You can see some samples building with this plugin at this <ulink url=\"http://digikamplugins.free.fr/ImagesGallerySamples/MultipleAlbums/FilterImages/index.html\"> <citetitle>url.</citetitle> </ulink>"
msgstr ""

